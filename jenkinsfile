pipeline {
    agent any
    tools {
       maven 'maven' 
    }
    stages { 
     
       stage('Clone') {
            steps {
              git url: 'https://github.com/Sanketjadhav31/Testing.git' 
            }
       }
            stage('Dependencies') {
                steps {
                    sh 'npm install'
            }                
            }      
        stage('Test') {
            steps {
               sh 'npm test || echo "No tests defined"'
            }
        }
        stage('bild') {
            steps {
                sh 'npm run build'
            }
        }

      }
    }
